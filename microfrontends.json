{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "megamind-blogs": {
      "routing": [
        {
          "paths": ["/", "/blogs", "/blogs/:path*"]
        }
      ]
    },
    "megamind-profile": {
      "routing": [
        {
          "paths": ["/profile", "/profile/:path*"]
        }
      ]
    }
  }
}